[manager]
swarm-manager ansible_host=10.0.1.20 ansible_user=ubuntu ansible_ssh_private_key_file=~/.ssh/yash-devops-key.pem

[workers]
worker-node-1 ansible_host=10.0.1.30 ansible_user=ubuntu ansible_ssh_private_key_file=~/.ssh/yash-devops-key.pem
worker-node-2 ansible_host=10.0.1.40 ansible_user=ubuntu ansible_ssh_private_key_file=~/.ssh/yash-devops-key.pem
